{
  "name": "rune-games-cli",
  "version": "2.0.3",
  "description": "Easy way to test your HTML5 game before releasing it on Rune.",
  "keywords": [
    "HTML5",
    "game",
    "javascript",
    "typescript"
  ],
  "type": "module",
  "repository": "git@github.com:rune/rune-games-cli.git",
  "author": "Rune AI Inc.",
  "bin": {
    "rune": "./dist/index.js"
  },
  "engines": {
    "node": ">=14.17"
  },
  "scripts": {
    "prepare": "husky install",
    "rune": "STAGE=local ts-node --esm src",
    "build": "rm -rf dist && tsc && chmod +x dist/index.js",
    "prepublishOnly": "yarn build",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "release": "scripts/release.sh",
    "gql-gen-local": "SCHEMA_PATH=http://localhost:3000/dev/graphql gql-gen"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@types/qrcode-terminal": "0.12.0",
    "adm-zip": "0.5.9",
    "apollo-upload-client": "17.0.0",
    "bson": "4.6.5",
    "core-js": "3.25.0",
    "cross-fetch": "3.1.5",
    "date-fns": "2.29.1",
    "express": "4.18.0",
    "figures": "4.0.1",
    "form-data": "4.0.0",
    "glob": "8.0.3",
    "graphql": "15.8.0",
    "ink": "3.2.0",
    "ink-spinner": "4.0.3",
    "ink-text-input": "4.0.3",
    "is-port-reachable": "4.0.0",
    "meow": "10.1.2",
    "mime-types": "2.1.35",
    "qrcode-terminal": "0.12.0",
    "react": "17.0.2",
    "semver": "7.3.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.1",
    "@graphql-codegen/named-operations-object": "2.2.0",
    "@graphql-codegen/schema-ast": "2.2.0",
    "@graphql-codegen/typed-document-node": "2.2.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "2.1.8",
    "@graphql-codegen/typescript-operations": "2.1.7",
    "@types/adm-zip": "0.5.0",
    "@types/apollo-upload-client": "17.0.1",
    "@types/express": "4.17.13",
    "@types/glob": "7.2.0",
    "@types/mime-types": "2.1.1",
    "@types/node": "17.0.25",
    "@types/react": "18.0.5",
    "@types/semver": "7.3.10",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.2",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "ts-node": "10.9.1",
    "typescript": "4.4.2"
  }
}
